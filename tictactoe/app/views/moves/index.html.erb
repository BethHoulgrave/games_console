<% title "Moves" %>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% for move in @moves %>
    <tr>
      <td><%= move.name %></td>
      <td><%= link_to "Show", move %></td>
      <td><%= link_to "Edit", edit_move_path(move) %></td>
      <td><%= link_to "Destroy", move, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Move", new_move_path %></p>